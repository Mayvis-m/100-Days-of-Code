# STRINGS (str)
first_name = "Mayvis"
last_name = "Underscore"
full_name = first_name +" "+ last_name

print("Hello "+ full_name)

#print(type(name))

# INTEGERS (int)
age = 100
age += 1
print("Your age is: "+str(age)) 
#print(type(age))

# FLOAT
height = 250.5
print("Your height is: "+str(height)+"cm")
#print(type(height))

# BOOLEAN
human = True
print("Are you a human: "+str(human))
# print(type(human))
